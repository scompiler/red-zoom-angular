{
  "name": "red-zoom",
  "version": "0.15.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-library": "ng build ngx-red-zoom --watch",
    "build-docs": "ng build demo --configuration production --base-href /red-zoom-angular/",
    "demo:build": "ng build demo --configuration production",
    "library:build": "ng build ngx-red-zoom --configuration production",
    "pack": "npm run library:build && npm run pack:scss",
    "pack:scss": "gulp pack",
    "ci:test": "ng test --no-watch --code-coverage --browsers ChromeHeadless --source-map true"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.0",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "github-buttons": "^2.29.1",
    "highlight.js": "^11.11.1",
    "ngx-owl-carousel-o": "^20.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.1",
    "@angular/cli": "^20.3.1",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "del": "^8.0.0",
    "gulp": "^5.0.1",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "typescript": "~5.9.2"
  }
}
